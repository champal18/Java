<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">

	<managed-bean>
		<managed-bean-name>PersonaBean</managed-bean-name>
		<managed-bean-class>controller.PersonaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>UsrBean</managed-bean-name>
		<managed-bean-class>controller.UsrBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>AdminBean</managed-bean-name>
		<managed-bean-class>controller.AdminBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

<navigation-rule> 
  <from-view-id>registro.xhtml</from-view-id> 
  <navigation-case>
    <from-outcome>exito</from-outcome>
      <to-view-id>/registroExitoso.xhtml</to-view-id> 
  </navigation-case>
</navigation-rule>

<navigation-rule> 
  <from-view-id>usuario_editarPerfil.xhtml</from-view-id> 
  <navigation-case>
    <from-outcome>editarUsr</from-outcome>
      <to-view-id>/usuario_perfil.xhtml</to-view-id> 
  </navigation-case>
</navigation-rule>

<navigation-rule> 
  <from-view-id>*</from-view-id> 
  <navigation-case>
    <from-outcome>usrPerfil</from-outcome>
      <to-view-id>/usuario_perfil.xhtml</to-view-id> 
  </navigation-case>
</navigation-rule>

<navigation-rule> 
  <from-view-id>*</from-view-id> 
  <navigation-case>
    <from-outcome>adminPerfil</from-outcome>
      <to-view-id>/admin_perfil.xhtml</to-view-id> 
  </navigation-case>
</navigation-rule>

</faces-config>
