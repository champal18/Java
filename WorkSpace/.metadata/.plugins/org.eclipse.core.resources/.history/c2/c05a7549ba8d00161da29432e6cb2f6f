package rest;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;
import java.util.Map;

import rest.Punto;

public enum puntoBuscado 
{
	instance;
	
	private Map<Long, Punto> puntoBuscado = new HashMap<Long,Punto>();

	public Map<Long, Punto> getPuntoBuscado() {
		return puntoBuscado;
	}

	public void setPuntoBuscado(Map<Long, Punto> puntoBuscado) {
		this.puntoBuscado = puntoBuscado;
	}
	
	public Punto getPunto()
	{
		List<Punto> puntoList = new ArrayList<Punto>();
		puntoList.addAll(puntoBuscado.values());
		return puntoList.get(0);
	}
	
	public boolean isEmpty()
	{
		if(puntoBuscado.isEmpty())
			return true;
		else
			return false;
	}


}
