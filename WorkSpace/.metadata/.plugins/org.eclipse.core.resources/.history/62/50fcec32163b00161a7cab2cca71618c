package rest;

import java.util.Map;

public class PuntoService 
{
	PuntoDao puntoDao;
	
	public PuntoService()
	{
		puntoDao = PuntoDao.instance;
	}
	
	public void createPunto(Punto p)
	{
		puntoDao.getPuntos().put(p.getId(), p);
	}
	
	public Map<Long, Punto> getPuntos()
	{
		return puntoDao.getPuntos();
	}

}
