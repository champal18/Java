package modelo;

import java.awt.Point;
import java.util.ArrayList;

import javax.persistence.*;

@Entity
public class Recorrido 
{
	@Id @GeneratedValue
	long id;
	private ArrayList<Point> puntos;
	
//	@OneToOne(optional = true, mappedBy = "ruta", cascade = {CascadeType.PERSIST, CascadeType.REMOVE, CascadeType.REFRESH})
	private Ruta ruta;
	
	public ArrayList<Point> getPuntos() 
	{
		return this.puntos;
	}

	public void setPuntos(ArrayList<Point> array) 
	{
		this.puntos = array;
	}
	
	pu

}